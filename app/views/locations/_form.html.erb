<%= form_for object do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br><br>
    <%= f.label :longitude %>
    <%= f.text_field :longitude %>
    <br><br>
    <%= f.label :latitude %>
    <%= f.text_field :latitude %>
    <br><br>
    <%= f.label :state %>
    <%= f.text_field :state %>
    <br><br>
    <%= f.label :distance %>
    <%= f.text_field :distance %>
    <br><br>
    <%= f.label :url %>
    <%= f.text_field :url %>
    <br><br>
    <%= f.label :season %>
    <%= f.text_field :season %>
    <br><br>
 
    <%= f.submit "Create" %>

<% end %>