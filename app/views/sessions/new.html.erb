<h1>Some cheese</h1>


<%= form_with(model: @user, url:'/login', local: true) do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %>
    <br><br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br><br>
    <%= f.submit "Sign In" %>
<% end %>

<%= button_to "Sign In with Google", '/auth/google_oauth2' %>