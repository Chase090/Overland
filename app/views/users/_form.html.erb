

<%= form_for object do |f|%>

<%= f.label "Profile Photo" %>
<%= f.text_field :avatar %>
<br><br>

<%= f.label :first_name %>
<%= f.text_field :first_name %>
<br><br>

<%= f.label :last_name %>
<%= f.text_field :last_name %>
<br><br>

<%= f.label :email %>
<%= f.text_field :email %>
<br><br>

<%= f.label :password %>
<%= f.password_field :password %>
<br><br>

<%= f.submit "Go ahead" %>




<%end%>